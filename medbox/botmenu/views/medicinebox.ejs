  <script>

        let medbox = MedBoxController({
            oneID : 123456,
            logEndpoint : 'https://anun.me:55550/cwznmll',
            timeout: 15000
        });
        function oneChatCallBackQRScanner(qrcode){
            medbox.openBoxByQRCode(qrcode.toString(), function (code, text) {
                alert(`${code} -- ${text}`);
            });
        }
    </script>

